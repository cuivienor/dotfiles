{
  "$schema": "https://raw.githubusercontent.com/streetsidesoftware/cspell/main/cspell.schema.json",
  "version": "0.2",
  "dictionaryDefinitions": [
    {
      "name": "project-words",
      "path": "./project-words.txt",
      "addWords": true
    },
    {
      "name": "personal",
      "path": "./personal.txt",
      "addWords": true
    }
  ],
  "dictionaries": ["project-words", "personal"],
  "overrides": [
    {
      "filename": "~/.local/share/nvim/gp/**/*.md",
      "enabled": false
    }
  ]
  "ignorePaths": ["node_modules"]
}

